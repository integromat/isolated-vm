{
  "name": "@integromat/isolated-vm",
  "version": "4.6.0",
  "description": "Access to multiple isolates - Make Fork",
  "main": "isolated-vm.js",
  "types": "isolated-vm.d.ts",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "install": "node-gyp-build",
    "rebuild": "node-gyp rebuild --release -j 4",
